@model Keas.Core.Domain.FinancialOrganization

@{
    ViewData["Title"] = "Add Org Code";
}
<div class="row justify-content-between">
  <div class="col-md-8">
  <div class="card">
    <div class="card-header-primary">
      <div class="card-head">
  <h2>Add Org Code</h2>
      </div>
    </div>
    <div class="card-content">
      <form asp-action="AddFISOrg">
          <div asp-validation-summary="ModelOnly" class="text-danger"></div>
          <div class="form-group">
              <label asp-for="Chart" class="control-label"></label>
              <input asp-for="Chart" class="form-control"/>
              <span asp-validation-for="Chart" class="text-danger"></span>
          </div>
          <div class="form-group">
              <label asp-for="OrgCode" class="control-label"></label>
              <input asp-for="OrgCode" class="form-control"/>
              <span asp-validation-for="OrgCode" class="text-danger"></span>
          </div>
          <div class="form-group">
              <i class="fas fa-plus blue fa-xs"/></i> <input type="submit" value="Add Org to Team" class="btn btn-link"/>
          </div>
      </form>
    </div>
  </div>
  </div>
  <div class="col-md-4">
    <div class="card">
      <div class="card-content">
        @{await Html.RenderPartialAsync("_TeamAdminMenu");}
      </div>
    </div>
  </div>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
