
@{
    ViewData["Title"] = "Upload CSV";
}

<div class="row justify-content-between">
    <div class="col-md-8">
        <div class="card">
            <div class="card-header-primary">
                <div class="card-head">
                    <h2>Upload Keys</h2>
                </div>
            </div>
            <div class="card-content">
                <a href="UploadKeys" class="btn btn-link">Upload Keys from CSV <i class="fas fa-chevron-right fa-sm" aria-hidden="true"></i></a>
            </div>
        </div>
        <div class="card">
            <div class="card-header-primary">
                <div class="card-head">
                    <h2>Upload Equipment</h2>
                </div>
            </div>
            <div class="card-content">
                <a href="UploadEquipment" class="btn btn-link">Upload Equipment from CSV <i class="fas fa-chevron-right fa-sm" aria-hidden="true"></i></a>
            </div>
        </div>
    </div>
    <div class="col-md-4">
        @{await Html.RenderPartialAsync("_TeamAdminMenu");}
    </div>
</div>


