@model Keas.Mvc.Controllers.TeamAdminController.TeamAdminMembersListModel

@{
    ViewData["Title"] = "Members " + Model.Team.Name;
}

<h2>Members of @Model.Team.Name </h2>

<table class="table">
    <thead>
        <tr>
            <th>
                First Name
            </th>
            <th>
                Last Name
            </th>
            <th>
                Roles
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var permission in Model.Team.TeamPermissions) {
        <tr>
            <td>
                @permission.User.FirstName
            </td>
            <td>
                @permission.User.LastName
            </td>
            <td>
            @permission.TeamRole.Name

            </td>
           
        </tr>
}
    </tbody>
</table>
